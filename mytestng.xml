<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suite">
    <test name="Functional Test">
        <classes>
            <class name="seleniumbasics.SeleniumBasics">
                <methods>
                    <include name="TC_001_verifyObsquraTitle"/>
                    <include name="TC_002_simpleInputForm"/>
                    <include name="TC_003_verifyTotal"/>
                    <exclude name="TC_004_verifyEmptyFieldsValidation"/>
                </methods>
            </class>
            <class name="seleniumbasics.Vehicle">
                <methods>
                    <include name="verifyVehicleQuality"/>
                    <include name="verifyvehicleBrand"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Non functional Test">
    <classes>
        <class name="seleniumbasics.SeleniumBasics">
            <methods>
                <include name="TC_005_verifyEmptyFieldCityStateValidation"/>
                <include name="TC_006_verifySubmitForm"/>
            </methods>
        </class>
    </classes>
</test>
</suite>
